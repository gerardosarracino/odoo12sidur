<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
<!--  Formulario de representante_ciudadano   -->
        <record id="firmas_logos_representante_ciudadano_view" model="ir.ui.view">
            <field name="name">Representante Ciudadano Form</field>
            <field name="model">firmas_logos.representante_ciudadano</field>
            <field name="arch" type="xml">
                <form string="representante ciudadano form">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="puesto"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!--  vista tree      -->
        <record id="firmas_logos_representante_ciudadano_tree_view" model="ir.ui.view">
            <field name="name">Representante Ciudadano Tree</field>
            <field name="model">firmas_logos.representante_ciudadano</field>
            <field name="arch" type="xml">
                <tree string="representante_ciudadano tree" >
                    <field name="name"/>
                    <field name="puesto"/>
                </tree>
            </field>
        </record>

        <!--  action view      -->
        <record id="action_firmas_logos_representante_ciudadano" model="ir.actions.act_window">
            <field name="name">Representante Ciudadano</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">firmas_logos.representante_ciudadano</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                Agregar Representante Ciudadano
              </p>
            </field>
        </record>
        <menuitem id="firmas_logos_representante_ciudadano" parent="firmas_logos" name="Representante Ciudadano"
                  action="action_firmas_logos_representante_ciudadano" sequence="1"/>
  </data>
</odoo>