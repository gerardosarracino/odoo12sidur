<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
<!--  Formulario de contraloria_interna     -->
        <record id="firmas_logos_contraloria_interna_view" model="ir.ui.view">
            <field name="name">Contraloria Interna Form</field>
            <field name="model">firmas_logos.contraloria_interna</field>
            <field name="arch" type="xml">
                <form string="contraloria interna form">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="puesto"/>
                            <field name="fecha_nombramiento"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!--  vista tree      -->
        <record id="firmas_logos_contraloria_interna_tree_view" model="ir.ui.view">
            <field name="name">Contraloria Interna Tree</field>
            <field name="model">firmas_logos.contraloria_interna</field>
            <field name="arch" type="xml">
                <tree string="contraloria interna tree" >
                    <field name="name"/>
                    <field name="puesto"/>
                    <field name="fecha_nombramiento"/>
                </tree>
            </field>
        </record>

        <!--  action view      -->
        <record id="action_firmas_logos_contraloria_interna" model="ir.actions.act_window">
            <field name="name">Contraloria Interna</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">firmas_logos.contraloria_interna</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                Agregar Contraloria Interna
              </p>
            </field>
        </record>
        <menuitem id="firmas_logos_contraloria_interna" parent="firmas_logos" name="Contraloria Interna"
                  action="action_firmas_logos_contraloria_interna" sequence="1"/>
  </data>
</odoo>