<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
<!--  Formulario de responsable_licitaciones   -->
        <record id="firmas_logos_responsable_licitaciones_view" model="ir.ui.view">
            <field name="name">responsable licitaciones Form</field>
            <field name="model">firmas_logos.responsable_licitaciones</field>
            <field name="arch" type="xml">
                <form string="responsable licitaciones form">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="puesto"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!--  vista tree      -->
        <record id="firmas_logos_responsable_licitaciones_tree_view" model="ir.ui.view">
            <field name="name">responsable licitacioneso Tree</field>
            <field name="model">firmas_logos.responsable_licitaciones</field>
            <field name="arch" type="xml">
                <tree string="responsable licitaciones tree" >
                    <field name="name"/>
                    <field name="puesto"/>
                </tree>
            </field>
        </record>

        <!--  action view      -->
        <record id="action_firmas_logos_responsable_licitaciones" model="ir.actions.act_window">
            <field name="name">Responsable Licitaciones</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">firmas_logos.responsable_licitaciones</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                Agregar Responsable Licitaciones
              </p>
            </field>
        </record>
        <menuitem id="firmas_logos_responsable_licitaciones" parent="firmas_logos"
                  name="Responsable Licitaciones"
                  action="action_firmas_logos_responsable_licitaciones" sequence="1"/>
  </data>
</odoo>